T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x153706430_0, 0, 1;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x153608db0;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "gtkmajority.vcd" {0 0 0};
